  "dependencies": {
    "approvejs": "^3.1.2",
    "chartist": "^0.11.0",
    "d3": "^5.5.0",
    "font-awesome": "^4.7.0",
    "ionicons": "^4.2.5",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "react": "^16.4.1",
    "react-chartist": "^0.13.1",
    "react-datamaps": "^0.4.1",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.4.1",
    "react-google-maps": "^9.4.5",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "^1.1.4",
    "react-select": "^2.0.0",
    "react-transition-group": "^2.4.0",
    "reactable": "^1.0.2",
    "reactstrap": "^6.3.0",
    "reapop": "^2.0.0",
    "recharts": "^1.1.0",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "source-map": "^0.7.3",
    "source-map-explorer": "^1.5.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.3.3",
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src/scss --include-path ./node_modules src/scss -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/scss --include-path ./node_modules src/scss -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build"
  }
}
